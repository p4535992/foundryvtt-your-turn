{
  "id": "your-turn",
  "title": "Your Turn! (Rikshy's fork)",
  "description": "A simple module which adds a user specific to do list.",
  "version": "1.00",
  "authors": [
    {
      "name": "Autmor"
    },
    {
      "name": "Rikshy"
    }
  ],
  "esmodules": [
    "./yourTurn.js"
  ],
  "styles": [
    "./styles/module.css"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    },
    {
      "lang": "de",
      "name": "German",
      "path": "lang/de.json"
    }
  ],
	"relationships": {
		"requires": [
			{
				"id": "color-picker",
				"type": "module",
				"manifest": "https://github.com/Larkinabout/fvtt-color-picker/releases/latest/download/module.json"
			}
		]
	},
  "url": "https://github.com/Autmor/yourTurn",
  "manifest": "",
  "download": "",
  "bugs": "https://github.com/Autmor/yourTurn/-/issues",
  "readme": "https://github.com/Autmor/yourTurn/blob/main/README.md",
  "flags": {
    "allowBugReporter": true
  },
  "compatibility": {
    "minimum": "10",
    "verified": "11"
  }
}